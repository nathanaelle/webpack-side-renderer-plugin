{
	"env": {
	    "node": false,
	    "es6": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
	    "ecmaFeatures": {
		"experimentalObjectRestSpread": true,
		"jsx": true
	    },
	    "sourceType": "module"
	},
	"plugins": [
	],
	"rules": {
	    "no-console": [
			"error",
			{ "allow": ["log","warn", "error"] }
		],
	    "indent": [
		"error",
		"tab"
	    ],
	    "linebreak-style": [
		"error",
		"unix"
	    ],
	    "quotes": [
		"error",
		"single"
	    ],
	    "semi": [
		"error",
		"never"
	    ]
	}
    }